{
    "version": 2,
    "builds": [{
      "src": "djangoprojectname/wsgi.py",
      "use": "vercel-sapper",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.8" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "znajdz_chwile/wsgi.py"
        }
    ]
}
